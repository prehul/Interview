    "def hdcf(x,y):\n",
    "    if x > y:\n",
    "        smaller = y\n",
    "    else:\n",
    "        smaller = x\n",
    "    for i in range(1,smaller+1):\n",
    "        if x % i ==0 and y % i ==0:\n",
    "            hcf  = i\n",
    "    print(hcf )\n",
    "    \n",
    "hdcf(54,24)     "
   ]
